<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckMenuItem?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Rectangle?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <MenuBar prefHeight="26.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <menus>
            <Menu mnemonicParsing="false" text="File">
               <items>
                  <MenuItem mnemonicParsing="false" text="Shuffle">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0">
                           <image>
                              <Image url="@../../img/suffle.png" />
                           </image>
                        </ImageView>
                     </graphic></MenuItem>
                  <MenuItem mnemonicParsing="false" text="Clear">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="X" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
                  <MenuItem mnemonicParsing="false" text="Exit" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Edit">
               <items>
                  <Menu mnemonicParsing="false" text="Picture Effect">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Sepia Tone" />
                        <MenuItem mnemonicParsing="false" text="Glow" />
                        <MenuItem mnemonicParsing="false" text="Shadow" />
                    </items>
                  </Menu>
                  <MenuItem disable="true" mnemonicParsing="false" text="No Effect" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="View">
               <items>
                  <CheckMenuItem mnemonicParsing="false" selected="true" text="Title" />
                  <CheckMenuItem mnemonicParsing="false" selected="true" text="Binomial name" />
                  <CheckMenuItem mnemonicParsing="false" selected="true" text="Picture" />
                  <CheckMenuItem mnemonicParsing="false" selected="true" text="Description" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
   </top>
   <left>
      <TabPane prefHeight="374.0" prefWidth="599.0" side="LEFT" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab disable="true" text="New">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
               </content>
            </Tab>
            <Tab text="Rectangle">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <ScrollPane hmax="50.0" layoutX="185.0" layoutY="87.0" prefHeight="200.0" prefWidth="200.0" vmax="50.0">
                          <content>
                            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <Rectangle arcHeight="5.0" arcWidth="5.0" height="500.0" smooth="false" stroke="#48ff00" strokeType="INSIDE" strokeWidth="5.0" width="500.0" />
                                 </children>
                              </AnchorPane>
                          </content>
                        </ScrollPane>
                     </children></AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </left>
</BorderPane>
